{
    "name": "Extension",
    "version": "1.0",
    "description": "Tracking your eyes!",
    "permissions": ["activeTab","declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["https://*/*"],
      "js": ["content.js","WebGazer.js"]
    }],
    "manifest_version": 2
  }