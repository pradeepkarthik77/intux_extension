{
  "manifest_version": 3,
  "name": "INTUX",
  "version": "1.0",
  "description": "Tracking to Improve!",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js",
        "WebGazer.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["crosshair.png","target.png","red_target.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "INTUX.png",
    "48": "INTUX.png",
    "128": "INTUX.png"
  }
}
